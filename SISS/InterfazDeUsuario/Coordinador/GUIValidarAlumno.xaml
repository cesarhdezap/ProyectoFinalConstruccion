<Window x:Class="InterfazDeUsuario.GUIsDeCoordinador.ValidarAlumno"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterfazDeUsuario.GUIsDeCoordinador"
        mc:Ignorable="d"
        Title="ValidarAlumno" Height="466" Width="888.875">
    <Grid>
        <Label x:Name="lblUniversidadVeracruzana" Content="Universidad Veracruzana" HorizontalAlignment="Left" VerticalAlignment="Top" Height="64" Width="882" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontWeight="Bold" FontFamily="Arial" FontSize="48" BorderThickness="2" BorderBrush="#FF110C0C" Margin="0,0,-90,0"/>
        <Label Content="Validar alumno" HorizontalAlignment="Left" Margin="94,69,0,0" VerticalAlignment="Top" Height="39" Width="157" FontFamily="Arial" FontSize="22" BorderBrush="#FF1B1414"/>
        <Label x:Name="LblNombreDeUsuario" Content="[Nombre de usuario]" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="627,23,0,0" Width="119"/>
        <Label x:Name="LblNombreDeUsuario_Copy" Content="Cerrar sesión" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="751,23,-41,0" Width="82" Foreground="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}" FontWeight="Bold"/>
        <DataGrid HorizontalAlignment="Left" Height="223" Margin="28,150,0,0" VerticalAlignment="Top" Width="805">
            <Button/>
        </DataGrid>
        <TextBox x:Name="TxtBuscar" HorizontalAlignment="Left" Height="23" Margin="616,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="217"/>
        <Label Content="Buscar proyecto por nombre:" HorizontalAlignment="Left" Margin="388,104,0,0" VerticalAlignment="Top" FontSize="16"/>
        <Button x:Name="BtnCancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="388,388,0,0" VerticalAlignment="Top" Width="108" FontFamily="Arial" FontSize="20"/>

    </Grid>
</Window>
