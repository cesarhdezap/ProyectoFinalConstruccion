<Window x:Class="InterfazDeUsuario.GUIsDeCoordinador.GUIBuscarProyecto"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterfazDeUsuario.GUIsDeCoordinador"
        mc:Ignorable="d"
        Title="GUIBuscarProyecto" Height="450" Width="890">
    <Grid>
        <Label x:Name="lblUniversidadVeracruzana" Content="Universidad Veracruzana" HorizontalAlignment="Left" VerticalAlignment="Top" Height="64" Width="882" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontWeight="Bold" FontFamily="Arial" FontSize="48" BorderThickness="2" BorderBrush="#FF110C0C" Margin="0,0,-90,0"/>
        <Label x:Name="LblBuscarProyecto" Content="Buscar proyecto" HorizontalAlignment="Left" Margin="100,69,0,0" VerticalAlignment="Top" Height="39" Width="173" FontFamily="Arial" FontSize="22" BorderBrush="#FF1B1414"/>
        <Label x:Name="LblNombreDeUsuario" Content="[Nombre de usuario]" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="627,23,0,0" Width="119"/>
        <Label x:Name="LblNombreDeUsuario_Copy" Content="Cerrar sesión" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="751,23,-41,0" Width="82" Foreground="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}" FontWeight="Bold"/>
        <Button x:Name="BtnCancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="375,382,0,0" VerticalAlignment="Top" Width="117" FontFamily="Arial" FontSize="20"/>
        <TextBox x:Name="TxtBuscarProyecto" HorizontalAlignment="Left" Height="23" Margin="627,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="230"/>
        <Label x:Name="LblBuscarProyectoPorNombre" Content="Buscar proyecto por nombre:" HorizontalAlignment="Left" Margin="385,108,0,0" VerticalAlignment="Top" Width="216" FontFamily="Arial" FontSize="16"/>
        <DataGrid x:Name="DtgProyectos" HorizontalAlignment="Left" Height="198" Margin="34,159,0,0" VerticalAlignment="Top" Width="813"/>

    </Grid>
</Window>
