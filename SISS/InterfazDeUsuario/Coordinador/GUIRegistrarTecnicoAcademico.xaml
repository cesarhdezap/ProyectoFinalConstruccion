        <Label Content="Nombre:" HorizontalAlignment="Left" Margin="276,152,0,0" VerticalAlignment="Top" FontSize="14" BorderBrush="#FF0E0B0B"/>
        <Label Content="Carrera:" HorizontalAlignment="Left" Margin="282,200,0,0" VerticalAlignment="Top" FontSize="14" BorderBrush="#FF0E0B0B"/>
        <Label Content="Correo electrónico:" HorizontalAlignment="Left" Margin="213,248,0,0" VerticalAlignment="Top" FontSize="14" BorderBrush="#FF0E0B0B"/>
        <Label Content="Confirmar correo electrónico:" HorizontalAlignment="Left" Margin="150,302,0,0" VerticalAlignment="Top" FontSize="14" BorderBrush="#FF0E0B0B"/>
        <Label Content="Contraseña:" HorizontalAlignment="Left" Margin="257,352,0,0" VerticalAlignment="Top" FontSize="14" BorderBrush="#FF0E0B0B" RenderTransformOrigin="0.687,0.621"/>
        <Label Content="Teléfono:" HorizontalAlignment="Left" Margin="274,401,0,0" VerticalAlignment="Top" FontSize="14" BorderBrush="#FF0E0B0B"/>
        <Label Content="Cubículo:" HorizontalAlignment="Left" Margin="273,450,0,0" VerticalAlignment="Top" FontSize="14" BorderBrush="#FF0E0B0B"/>
        <TextBox x:Name="TxtNombre" HorizontalAlignment="Left" Height="23" Margin="398,152,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" FontSize="14"/>
        <ComboBox x:Name="CbxCarrera" HorizontalAlignment="Left" Margin="398,200,0,0" VerticalAlignment="Top" Width="262" FontSize="14"/>
        <TextBox x:Name="TxtCorreoElectronico" HorizontalAlignment="Left" Height="24" Margin="398,248,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" FontSize="14"/>
        <TextBox x:Name="TxtConfirmarCorreoElectronico" HorizontalAlignment="Left" Height="23" Margin="398,302,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" FontSize="14"/>
        <TextBox x:Name="TxtContraseña" HorizontalAlignment="Left" Height="23" Margin="398,352,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" FontSize="14"/>
        <TextBox x:Name="TxtTelefono" HorizontalAlignment="Left" Height="24" Margin="398,401,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" FontSize="14"/>
        <TextBox x:Name="TxtCubiculo" HorizontalAlignment="Left" Height="23" Margin="398,450,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" FontSize="14"/>
