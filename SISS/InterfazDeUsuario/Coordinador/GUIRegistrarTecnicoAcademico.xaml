        <Label Content="Registrar técnico académico" HorizontalAlignment="Left" Margin="301,69,0,0" VerticalAlignment="Top" Height="39" Width="288" FontFamily="Arial" FontSize="22" BorderBrush="#FF1B1414"/>
        <Label x:Name="LblNombreDeUsuario" Content="[Nombre de usuario]" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="627,23,0,0" Width="119"/>
        <Label x:Name="LblNombreDeUsuario_Copy" Content="Cerrar sesión" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="751,23,0,0" Width="82" Foreground="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}" FontWeight="Bold"/>
        <Label Content="Nombre:" HorizontalAlignment="Left" Margin="202,150,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="20" BorderBrush="#FF0E0B0B"/>
        <Label Content="Carrera:" HorizontalAlignment="Left" Margin="206,197,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="20" BorderBrush="#FF0E0B0B"/>
