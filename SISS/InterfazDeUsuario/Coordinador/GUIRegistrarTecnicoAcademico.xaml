        <TextBox x:Name="TxtContraseña" HorizontalAlignment="Left" Height="23" Margin="328,355,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="261"/>
        <TextBox x:Name="TxtTelefono" HorizontalAlignment="Left" Height="24" Margin="328,404,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="261"/>
        <TextBox x:Name="TxtCubiculo" HorizontalAlignment="Left" Height="23" Margin="328,453,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="261"/>
        <Button x:Name="BtnCancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="235,524,0,0" VerticalAlignment="Top" Width="91" FontFamily="Arial" FontSize="20"/>
        <Button x:Name="BtnAceptar" Content="Aceptar" HorizontalAlignment="Left" Margin="522,524,0,0" VerticalAlignment="Top" Width="91" FontFamily="Arial" FontSize="20"/>
        <ComboBox x:Name="CbxCarrera" HorizontalAlignment="Left" Margin="328,203,0,0" VerticalAlignment="Top" Width="261"/>

