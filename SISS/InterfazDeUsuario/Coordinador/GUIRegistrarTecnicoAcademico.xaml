        <Label Content="Contraseña:" HorizontalAlignment="Left" Margin="170,349,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="20" BorderBrush="#FF0E0B0B"/>
        <Label Content="Teléfono:" HorizontalAlignment="Left" Margin="198,398,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="20" BorderBrush="#FF0E0B0B"/>
        <Label Content="Cubículo:" HorizontalAlignment="Left" Margin="195,448,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="20" BorderBrush="#FF0E0B0B"/>
        <TextBox x:Name="TxtNombre" HorizontalAlignment="Left" Height="23" Margin="328,155,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="261"/>
        <TextBox x:Name="TxtCorreoElectronico" HorizontalAlignment="Left" Height="24" Margin="328,251,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="261"/>
