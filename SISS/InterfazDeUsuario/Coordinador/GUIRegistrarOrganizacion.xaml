        <Label Content="Estado:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="272,247,0,0" FontFamily="Arial" FontSize="20"/>
        <Label Content="Ciudad:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="271,293,0,0" FontFamily="Arial" FontSize="20"/>
